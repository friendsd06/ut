def _setup_cache(self) -> None:
        """Create cache file if it doesn't exist and ensure proper permissions."""
        try:
            # Create parent directories if they don't exist
            self._cache_file.parent.mkdir(parents=True, exist_ok=True)
            
            # Create file if it doesn't exist with empty token dictionary
            if not self._cache_file.exists():
                logging.info("Cache file not found. Creating new cache file...")
                empty_cache = {
                    'access_token': None,
                    'expiration': None,
                    'username': self.username
                }
                with open(self._cache_file, 'w') as f:
                    json.dump(empty_cache, f)
                logging.info("Created new cache file")
            
            # Set proper permissions
            os.chmod(self._cache_file, stat.S_IRUSR | stat.S_IWUSR)  # 600 permissions
            os.chown(self._cache_file, os.getuid(), os.getgid())
            logging.info("Cache file permissions set")
            
        except Exception as e:
            logging.error(f"Failed to setup cache file: {e}")
            raise RuntimeError(f"Cache initialization failed: {str(e)}") from e

            -----------------------------------------------------------------------
            %JBOSS_HOME%\bin\add-user.bat -a wbadmin admin@123 -g admin,analyst
            wbadmin: Username (you can use any name).
            admin@123: Password (replace with a strong password).
            admin,analyst: Roles assigned to the user.